get_country_change <- function(country_name, data_frame_LE){
country_LE_1968 <- data_frame_LE$X1968(data_frame_LE$country == country_name)
country_LE_2018 <- data_frame_LE$X2018(data_frame_LE$country == country_name)
return(country_LE_2018-country_LE_1968)
}
